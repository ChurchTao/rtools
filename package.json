{
  "name": "rtools",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tauri-apps/api": "^1.2.0",
    "pinia": "^2.0.35",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.45.2",
    "@tauri-apps/cli": "^1.2.3",
    "@types/node": "^18.7.10",
    "@vitejs/plugin-vue": "^4.0.0",
    "consola": "^3.1.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.11.0",
    "less": "^4.1.3",
    "lint-staged": "^13.2.1",
    "prettier": "2.8.7",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.0.11"
  }
